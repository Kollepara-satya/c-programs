#include <stdio.h>
#include <stdlib.h>
int main()
{
   int *a,i,n,sum=0;
   printf("enter size:");
   scanf("%d",&n);
   a=(int *)malloc(n*sizeof(int));
   printf("\nenter elements into array:");
   for(i=0;i<n;i++)
     scanf("%d",a+i);
    for(i=0;i<n;i++)
     sum=sum+*(a+i);
    printf("%d",sum); 
   
    return 0;
}
