
#include<stdio.h>
void main()
    {
        char str[100];
        int i;
        printf("enter a string:");
        gets(str);
        for(i=0;str[i]!='\0';i++)
        {
            if(i%2!=0)
            {
                str[i]=str[i]-32;
            }
        }
        puts(str);
        
        
    }
