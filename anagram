#include <stdio.h>
void main()
{
    char s1[50],s2[50];
    int i,j,temp,n1,n2,result;
    printf("enter string 1:\t");
    gets(s1);
    printf("enter string 2:\t");
    gets(s2);
    n1=strlen(s1);
    n2=strlen(s2);
    for(i=0;i<n1-1;i++)
    {
        for(j=0;j<n1-i-1;j++)
        {
            if(s1[j]>s1[j+1])
            {
              temp=s1[j];
              s1[j]=s1[j+1];
              s1[j+1]=temp;
            }
        }
    }
    for(i=0;i<n2-1;i++)
    {
        for(j=0;j<n2-i-1;j++)
        {
            if(s2[j]>s2[j+1])
            {
              temp=s2[j];
              s2[j]=s2[j+1];
              s2[j+1]=temp;
            }
        }
    }
    result=strcmp(s1,s2);
    if(result==0)
    printf("\nanagrams");
    else
    printf("\nnot anagrams");
} 
    
    
    
    
    
    
